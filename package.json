{
  "name": "react-package-template",
  "version": "1.0.0",
  "description": "react npm package template",
  "keywords": [
    "webpack",
    "rollup"
  ],
  "author": "yehuda goldberg",
  "repository": {
    "type": "git",
    "url": "https://"
  },
  "license": "MIT",
  "main": "dist/bundle.cjs.js",
  "module": "dist/bundle.esm.js",
  "scripts": {
    "clean": "rimraf ./dist",
    "example:build": "npm run example:clean && webpack --mode production",
    "example:clean": "rimraf ./example/dist",
    "example:start": "webpack-dev-server --mode development",
    "lint": "eslint example/src/ src/ --ext .jsx,.js --fix",
    "prepack": "npm run clean && npm run transpile",
    "transpile": "rollup -c",
    "watch": "rollup -c -w"
  },
  "peerDependencies": {
    "react": "^16.13.1"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.12.13",
    "prop-types": "^15.7.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.13",
    "@babel/core": "^7.12.13",
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@babel/plugin-transform-runtime": "^7.12.15",
    "@babel/preset-env": "^7.12.13",
    "@babel/preset-react": "^7.12.13",
    "@rollup/plugin-babel": "^5.2.3",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@rollup/plugin-url": "^6.0.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "css-loader": "^3.6.0",
    "eslint": "^6.8.0",
    "eslint-import-resolver-webpack": "^0.13.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "html-webpack-plugin": "^4.5.1",
    "less-loader": "^8.0.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.38.5",
    "rollup-plugin-less": "^1.1.3",
    "style-loader": "^1.3.0",
    "url-loader": "^4.1.1",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.2"
  }
}
